{
  "name": "todo-the-game",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "date-fns": "^1.29.0",
    "firebase": "^5.2.0",
    "firebaseui": "^3.1.1",
    "micro": "latest",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-transition-group": "^1.2.1"
  },
  "scripts": {
    "start": "REACT_APP_TDG_PROJECT_ID=$TDG_PROJECT_ID REACT_APP_TDG_API_KEY=$TDG_API_KEY react-scripts start",
    "build": "REACT_APP_TDG_PROJECT_ID=$TDG_PROJECT_ID REACT_APP_TDG_API_KEY=$TDG_API_KEY react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "now": {
    "alias": "todo-the-game",
    "env": {
      "TDG_API_KEY": "@tdg_api_key",
      "TDG_PROJECT_ID": "@tdg_project_id"
    }
  }
}
